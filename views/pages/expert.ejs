    <!-- header -->
    <header>
        <!-- header inner -->
        <div class="header">

            <div class="container">

                <div class="row">

                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-3 col logo_section">
                        <div class="full">
                            <div class="center-desk">

                                <div class="logo">
                                    <a href="/index"><img src="images/logo.png"class="rounded float-left" alt="#" style="margin-left: -50%;"></a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-9 pt-3 pt-5">
                        <div class="menu-area">
                            <div class="limit-box">
                                <nav class="main-menu">
                                    <ul class="menu-area-main" style="margin-left: -17%;">
                                        <li class="active nav-btn"> <a href="/index" class="text-muted">Home</a> </li>
                                        <li class="active nav-btn"> <a href="/about" class="text-muted">About Us</a> </li>
                                        <li class ="active nav-btn"><a href="/brand" class="text-muted">Products</a></li>
                                        <li class="active nav-btn"><a href="/cart" class="text-muted">My Cart</a></li>
                                        <li class="active nav-btn"><a href="/order" class="text-muted">My Order</a></li>
                                        <li class="active nav-btn"><a href="/expert" class="text-muted">Experts</a></li>
                                        <!--
                                        <li class="active nav-btn"><a href="/forum" class="text-muted">Forum</a></li>
                                        -->
                                        <li class="active nav-btn"><a href="/account" class="text-muted">Log In</a></li>
                                    </ul>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- end header inner -->
    </header>



    
    
    <!--Main content-->
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="main-box clearfix">
                    <div class="table-responsive">
                        <table class="table user-list">
                            <thead>
                                <tr>
                                    <th class="text-center h5"><span>Name</span></th>
                                    <!--
                                    <th class="text-center h5"><span>Company</span></th>
                                    -->
                                    <!--
                                    <th class="text-center h5"><span>Status</span></th>
                                    -->
                                    <th class="text-center h5"><span>Expertise</span></th>
                                    <th class="text-center h5"><span>Email</span></th>
                                </tr>
                            </thead>
                            <tbody>
                                <% result.forEach((item)=>{ %>
                                <tr>


                                    <!--Name-->
                                    <td>
                                        <img src="https://bootdey.com/img/Content/avatar/avatar1.png" alt="">
                                        <a href="#" class="user-link"><%=item.expert_name%></a>
                                        <span class="user-subhead">Experienced</span>
                                    </td>



                                    <!--Company-->
                                    <!--
                                    <td class="text-center" style="font-size: 125%;">
                                        Square
                                    </td>
                                    -->


                                    <!--Status-->
                                    <!--
                                    <td class="text-center" style="font-size: 125%;">
                                        <span class="label label-default">Active</span>
                                    </td>
                                    -->


                                    <!--Expertise-->
                                    <td class="text-center" style="font-size: 110%;">
                                        <span class="label label-default"><%=item.expertise%></span>
                                    </td>
                                    


                                      <!--Email-->
                                      <td class="text-center">
                                        <a href = "mailto: fjSopu@yahoo.com" class="fa fa-envelope" style="font-size: 150%;"><%=item.expert_email_id%></a>
                                    </td>


                                </tr>
                                <%})%>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>






    

    <!--Footer-->
